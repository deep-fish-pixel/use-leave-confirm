{
  "name": "vue-demo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "vue-tsc && vite build",
    "dev": "vite",
    "lint": "pnpm lint:eslint && pnpm lint:prettier",
    "lint:eslint": "eslint --cache --max-warnings 0 \"src/**/*.{vue,js,ts,tsx}\" --fix",
    "lint:prettier": "eslint . --fix",
    "prepare": "husky install",
    "start": "npm run dev"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts}": [
      "eslint src"
    ]
  },
  "dependencies": {
    "@nio-fe/lego": "^1.4.1",
    "@types/nprogress": "^0.2.0",
    "axios": "^1.3.3",
    "keep-alive-vue3": "^1.2.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.30",
    "use-leave-confirm": "^1.1.2",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@commitlint/cli": "^17.4.3",
    "@commitlint/config-conventional": "^17.4.3",
    "@nio-fe/eslint-config-nio-vue": "^1.0.14",
    "@types/node": "^18.13.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "husky": "^8.0.3",
    "jsdom": "^21.1.0",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "sass": "^1.58.3",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-plugin-mock-dev-server": "^0.3.21",
    "vue-tsc": "^1.0.24"
  }
}
